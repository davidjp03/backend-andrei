{
  "family": "parcial-backend-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::476114140374:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "parcial-backend-app",
      "image": "476114140374.dkr.ecr.us-east-1.amazonaws.com/parcial-backend:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "DATABASE_URL",
          "value": "postgresql://postgres.wuaklwmawrgklpyvyolx:bUXo9mMGDE0ixdqA@aws-1-us-east-1.pooler.supabase.com:6543/postgres"
        },
        {
          "name": "JWT_SECRET",
          "value": "your-super-secret-jwt-key-change-this-in-production"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/parcial-backend",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}